{
	"name": "dan.my.id",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "pnpm /^dev:.*/",
		"dev:glide": "paraglide-js compile --project ./project.inlang --watch",
		"dev:vite": "vite dev --port=3000 && glide:watch",
		"build": "pnpm /^build:.*/",
		"build:glide": "paraglide-js compile --project ./project.inlang",
		"build:vite": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@fastkit/accept-language": "^0.13.7",
		"@fontsource/source-code-pro": "^5.0.16",
		"@fontsource/source-sans-3": "^5.0.18",
		"@fontsource/source-serif-4": "^5.0.16",
		"@importable/yaml": "^0.1.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.4.24",
		"highlight.js": "^11.9.0",
		"highlightjs-svelte": "^1.0.6",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.32",
		"postcss-import": "^15.1.0",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-figure": "^1.0.1",
		"rehype-link-processor": "^1.0.4",
		"rehype-slug": "^6.0.0",
		"rehype-stringify": "^10.0.0",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.0",
		"sveltekit-localize-url": "^0.1.2",
		"tailwindcss": "^3.4.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
